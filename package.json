{
  "name": "fourcast",
  "private": true,
  "version": "0.0.0",
  "main": "electron/electron.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "react-scripts start",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron electron/electron.cjs",
    "electron:pack": "electron-builder --dir",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run dev\" \"npm run electron\" \"python src/sync.py\"",
    "build:for:electron": "tsc --noEmit && cross-env ELECTRON=true vite build",
    "app:build": "npm run build:for:electron && npm run electron:builder"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.893.0",
    "@heroicons/react": "^2.2.0",
    "@material-tailwind/react": "^2.1.10",
    "apexcharts": "^5.3.5",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "jszip": "^3.10.1",
    "react": "^19.1.1",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.13",
    "tw-elements-react": "^1.0.0-alpha-end",
    "wait-on": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "electron": "^38.1.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6"
  }
}
